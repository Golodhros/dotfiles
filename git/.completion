#   Git Autocomplete
#   To Setup:
#   1) Save the .git-completion.bash file found in this folder into the home folder
#
#   Reference: https://github.com/git/git/blob/master/contrib/completion/git-completion.bash
if [ -f ~/.git-completion.bash ]; then
    . ~/.git-completion.bash

#   Add git completion to aliases
    __git_complete gs git status -s
    __git_complete gb git branch
    __git_complete gt git checkout
    __git_complete gd git diff
    __git_complete gc git commit
fi
